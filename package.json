{
  "name": "postcss-theme-customization",
  "version": "0.2.2",
  "description": "PostCSS plugin for theme customization",
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "postcss-theme-customization"
  ],
  "scripts": {
    "test": "node index.test.js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "initialChangelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "postversion": "git push && git push origin --tags"
  },
  "author": "qiuzhengqiang",
  "license": "MIT",
  "repository": "zhengqiang/postcss-theme-customization",
  "engines": {
    "node": ">=12.0.0"
  },
  "peerDependencies": {
    "postcss": "^7.0.39"
  },
  "devDependencies": {
    "clean-publish": "^3.4.2",
    "eslint": "^8.0.1",
    "postcss": "^7.0.39",
    "conventional-changelog-cli": "^2.2.2",
    "cz-conventional-changelog": "^3.3.0"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2017
    },
    "env": {
      "node": true,
      "es6": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:jest/recommended"
    ],
    "rules": {
      "jest/expect-expect": "off"
    }
  }
}
